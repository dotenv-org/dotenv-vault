{
  "name": "dotenv-vault",
  "version": "1.18.0",
  "description": "Sync your .env files, securely.",
  "author": "Mot @dotenvorg",
  "bin": {
    "dotenv-vault": "./bin/run"
  },
  "homepage": "https://github.com/dotenv-org/dotenv-vault",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "dotenv-org/dotenv-vault",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5.2.6",
    "@oclif/plugin-not-found": "^2.3.13",
    "@oclif/plugin-update": "^3.1.3",
    "@oclif/plugin-warn-if-update-available": "^2.0.24",
    "abortcontroller-polyfill": "1.7.5",
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "dotenv": "16.0.3",
    "dotenv-vault-core": "0.7.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.18.11",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "fs-monkey": "^1.0.3",
    "globby": "^11",
    "jest": "^27.5.1",
    "memfs": "^3.4.13",
    "mocha": "^9",
    "oclif": "^2",
    "shx": "^0.3.4",
    "sinon": "^13.0.2",
    "spyfs": "^1.0.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "4.9.5"
  },
  "oclif": {
    "bin": "dotenv-vault",
    "dirname": "dotenv-vault",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 1
    },
    "topicSeparator": " ",
    "topics": {},
    "update": {
      "node": {
        "version": "14.19.0"
      },
      "s3": {
        "xz": false,
        "bucket": "dotenv-vault"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/dotenv-org/dotenv-vault/issues",
  "keywords": [
    "dotenv",
    "dotenv-vault",
    "env",
    ".env",
    "environment",
    "variables",
    "config",
    "settings"
  ],
  "types": "dist/index.d.ts"
}
